package application;

import java.util.ArrayList;
import java.util.List;

public class ManipulandoList {

	public static void main(String[] args) {

		List<String> list = new ArrayList<>();// List é uma interface, por isso é instanciada através da classe
												// ArrayList

		list.add("Maria");
		list.add("Alex");
		list.add("Bob");
		list.add("Anna");
		list.add(2, "Marco");

		System.out.println(list.size());
		
		for(String x : list)
	}

}
